{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "parLocation": {
      "value": "westeurope"
    },
    "parCompanyPrefix": {
      "value": "jawas"
    },
    "parAzFirewallTier": {
      "value": "Standard"
    },
    "parVirtualHubEnabled": {
      "value": true
    },
    "parAzFirewallDnsProxyEnabled": {
      "value": true
    },
    "parVirtualWanName": {
      "value": "jawas-vwan-westeu"
    },
    "parVirtualWanHubName": {
      "value": "jawas-vhub"
    },
    "parVpnGatewayName": {
      "value": "jawas-vpngw"
    },
    "parExpressRouteGatewayName": {
      "value": "jawas-ergw"
    },
    "parAzFirewallName": {
      "value": "jawas-fw"
    },
    "parAzFirewallAvailabilityZones": {
      "value": []
    },
    "parAzFirewallPoliciesName": {
      "value": "jawas-azfwpolicy-westeu"
    },
    "parVirtualWanHubs": {
      "value": [
        {
          "parVpnGatewayEnabled": true,
          "parExpressRouteGatewayEnabled": true,
          "parAzFirewallEnabled": true,
          "parVirtualHubAddressPrefix": "10.100.0.0/23",
          "parHubLocation": "westeurope",
          "parHubRoutingPreference": "ExpressRoute",
          "parVirtualRouterAutoScaleConfiguration": 2
        }
      ]
    },
    "parVpnGatewayScaleUnit": {
      "value": 1
    },
    "parExpressRouteGatewayScaleUnit": {
      "value": 1
    },
    "parDdosEnabled": {
      "value": true
    },
    "parDdosPlanName": {
      "value": "jawas-ddos-plan"
    },
    "parTags": {
      "value": {
        "Environment": "Live"
      }
    }
  }
}
